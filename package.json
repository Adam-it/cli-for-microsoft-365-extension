{
	"name": "cli-for-microsoft-365-extension",
	"displayName": "CLI for Microsoft 365 extension",
	"description": "This extension adds snippets with all possible commands from CLI for Microsoft 365",
	"version": "1.5.8",
	"publisher": "adamwojcikit",
	"author": {
		"name": "Adam"
	},
	"icon": "assets/icon.png",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Snippets",
		"Programming Languages",
		"Other"
	],
	"keywords": [
		"CLI",
		"Microsoft365",
		"SharePoint"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Adam-it/cli-for-microsoft-365-extension"
	},
	"contributes": {
		"snippets": [
			{
				"language": "shellscript",
				"path": "./snippets/cliForMicrosoft365.code-snippets"
			},
			{
				"language": "powershell",
				"path": "./snippets/cliForMicrosoft365.code-snippets"
			},
			{
				"language": "javascript",
				"path": "./snippets/cliForMicrosoft365.code-snippets"
			}
		],
		"grammars": [
			{
				"language": "shellscript",
				"scopeName": "cliM365.ps1",
				"path": "./syntaxes/cliForM365.tmGrammar.json"
			},
			{
				"language": "powershell",
				"scopeName": "cliM365.ps1",
				"path": "./syntaxes/cliForM365.tmGrammar.json"
			},
			{
				"language": "javascript",
				"scopeName": "cliM365.ps1",
				"path": "./syntaxes/cliForM365.tmGrammar.json"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "cliM365",
					"title": "CLI for Microsoft 365 commands",
					"icon": "assets/logo.svg"
				}
			]
		},
		"views": {
			"cliM365": [
				{
					"type": "webview",
					"id": "cliM365",
					"name": "CLI for Microsoft 365 commands",
					"icon": "assets/logo.svg"
				}
			]
		}
	},
	"activationEvents": [
		"onView:cliM365"
	],
	"main": "./out/extension.js",
	"scripts": {
		"install:all": "npm install && npm --prefix ./webview-ui/activityBar install ./webview-ui/activityBar",
		"start:webview": "npm --prefix ./webview-ui/activityBar run start",
		"build:webview": "npm --prefix ./webview-ui/activityBar run build",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"clean": "del-cli --force ./out"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"del-cli": "^4.0.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"prettier": "^2.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}